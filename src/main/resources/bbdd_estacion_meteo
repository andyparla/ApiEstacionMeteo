CREATE TABLE ESTA_METEO_DATA (
    ID BIGINT UNSIGNED AUTO_INCREMENT PRIMARY KEY,
    CHIP_ID VARCHAR(50),
    TEMPERATURA VARCHAR(5),
    HUMEDAD VARCHAR(5),
    HIC VARCHAR(5),
    FEC_GRABADO TIMESTAMP NULL 
);

GRANT USAGE ON *.* TO 'andyparla'@'%' IDENTIFIED BY 'Trasto,.01';

GRANT ALL privileges ON `ESTA_METEO_DATA`.* TO 'andyparla'@localhost;
