CREATE TABLE ESTA_METEO_DATA (
    ID BIGINT UNSIGNED AUTO_INCREMENT PRIMARY KEY,
    CHIP_ID VARCHAR(50),
    TEMPERATURA VARCHAR(5),
    HUMEDAD VARCHAR(5),
    HIC VARCHAR(5),
    FEC_GRABADO TIMESTAMP NULL 
);

sudo mysql -uroot -p
--Trasto

GRANT USAGE ON *.* TO 'andyparla'@'%' IDENTIFIED BY 'Trasto,.01';

GRANT ALL privileges ON `ESTA_METEO_DATA`.* TO 'andyparla'@localhost;
GRANT ALL privileges ON `user`.* TO 'andyparla'@localhost;


CREATE TABLE USER (
    id BIGINT UNSIGNED AUTO_INCREMENT PRIMARY KEY,
    username VARCHAR (50) NOT NULL UNIQUE,
    password VARCHAR(50),
    email VARCHAR(100),
    created_at TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
    updated_at TIMESTAMP ON UPDATE CURRENT_TIMESTAMP
    --CONSTRAINT username_unique UNIQUE (username) 
);
